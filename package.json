{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test:unit": "jest",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier -w \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larahbarbosa/connected-waves.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Larissa Barbosa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/larahbarbosa/connected-waves/issues"
  },
  "homepage": "https://github.com/larahbarbosa/connected-waves#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.13",
    "axios": "^1.3.4",
    "axios-version": "^1.0.0",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "16",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "prettier": "^2.8.6",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
